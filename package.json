{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "Todo list built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heruku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisragasa/todo-list.git"
  },
  "author": "Christopher Ragasa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisragasa/todo-list/issues"
  },
  "homepage": "https://github.com/chrisragasa/todo-list#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.15"
  },
  "devDependencies": {
    "nodemon": "^1.18.7"
  }
}
